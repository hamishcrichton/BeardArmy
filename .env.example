YOUTUBE_API_KEY=your_youtube_api_key
YOUTUBE_CHANNEL_ID=UCc9CjaAjsMMvaSghZB7-Kog  # BeardMeatsFood channel ID

# Default to SQLite (no external DB server required)
DATABASE_URL=sqlite:///./data/app.db
# For Postgres (optional):
# DATABASE_URL=postgresql+psycopg://user:password@localhost:5432/bmf

